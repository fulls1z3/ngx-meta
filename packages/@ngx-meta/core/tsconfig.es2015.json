{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "importHelpers": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "removeComments": true,
    "stripInternal": true,
    "outDir": "../../../../.temp/packages/@ngx-meta/core-es2015/",
